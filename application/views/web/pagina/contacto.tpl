{strip}

<section class="sect-contacto">
	<div class="container height">
		<div class="row height align-items-center">
			<div class="col-lg-6">
				<div class="sect-contacto-text">
					<p class="contacto-text1"></p>
					<p class="contacto-text2">Contáctenos</p>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="sect-contacto2">
	<div class="container">
		<div class="row">
			<div class="col-lg-4">
				<div class="seccion-contenedor">
					<a href="#">
						<span><i class="fa fa-location-arrow"></i></span>
						<div class="content">
							<h5>Dirección:</h5>
							<h6>Av. Alisos Cdra 5 - Los Olivos</h6>
						</div>
					</a>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="seccion-contenedor">
					<a href="#">
						<span><i class="fa fa-phone"></i></span>
						<div class="content">
							<h5>Celular:</h5>
							<h6>(+51) 993 765 495</h6>
						</div>
					</a>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="seccion-contenedor">
					<a href="#">
						<span><i class="fa fa-clock-o"></i></span>
						<div class="content">
							<h5>Horario de Atención:</h5>
							<h6>Lunes-Viernes: 08:00 – 17:00</h6>
						</div>
					</a>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="sect-contacto3">
	<div class="container">
		<div class="row justify-content-center">
			<div class="col-lg-6">
                <form>
                	<div class="row">
	                    <div class="col-lg-6">
	                        <div class="form-group">
	                             <input type="text" class="form-control imput-color" placeholder="Nombres y Apellidos">
	                        </div>
	                    </div>
	                    <div class="col-lg-6">
	                        <div class="form-group">
	                             <input type="text" class="form-control imput-color" placeholder="Celular">
	                        </div>
	                    </div>
	                    <div class="col-lg-6">
	                        <div class="form-group">
	                             <input type="email" class="form-control imput-color" placeholder="Email">
	                        </div>
	                    </div>
	                    <div class="col-lg-6">
		                    <div class="form-group">
		                         <input type="text" class="form-control imput-color" placeholder="Asunto">
		                    </div>
		                </div>
		                <div class="col-lg-12">
		                    <div class="form-group">
		                        <textarea class="form-control imput-color" rows="4" placeholder="Mensaje"></textarea>
		                    </div>
		                </div>
	                    <div class="col-lg-12">
		                    <div class="text-center">
		                        <button class="btn btn-orange">Enviar</button>
		                    </div>
		                </div>
		            </div>
                </form>
            </div>
		</div>
	</div>
</section>
<section>
	<div>
		<div id="map" style="width: 100%; height: 400px;"></div>
	</div>
</section>
{/strip}